  "files": [
    "index.js",
    "route.js"
  ],
  "devDependencies": {
    "@architect/architect": "^10.10.3",
    "@architect/eslint-config": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "proxyquire": "^2.1.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.3"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}
